<?php

function contact_edit_form_alter(&$form, $form_state, $form_id) {
    if ($form_id == 'contact_site_form'){
        print '<pre>';
        print_r($form);
        print '</pre>';
        $form['copy']['#access'] = 0;
        $form['copy']['#default_value'] = 1;
        $form['subject']['#default_value'] = t('Feedback');
        $form['subject']['#access'] = 0;
    }

}
